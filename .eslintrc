{
  "extends": ["wesbos"],
  "rules": {
    "no-plusplus": 0,
    "no-console": 1,
    "no-var": 0,
    "no-shadow": 1,
    "react/prop-types": false,
    "react-hooks/exhaustive-deps": 2,
    "jsx-a11y/no-onchange": 2,
    "jsx-a11y/label-has-for": 0,
    "react/jsx-key": 2,
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "printWidth": 80
      }
    ]
  }
}
